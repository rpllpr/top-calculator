<!DOCTYPE html>
<html>
    <head>
        <title>rpllpr Calculator</title>
        <meta charset="utf-8"/>
        <link rel="stylesheet" type="text/css" href="style.css"/> 
        <script defer src="script.js"></script>
    </head>
    <body>
        <div class="calculator-frame">
            <div class="calculator">
                <div class="display">
                    <p class="display-p"></p>
                </div>
                <div class="button-div">CE</div>
                <div class="button-div">/</div>
                <div class="button-div">*</div>
                <div class="button-div">-</div>
                <div class="button-div">7</div>
                <div class="button-div">8</div>
                <div class="button-div">9</div>
                <div class="button-div add">+</div>
                <div class="button-div">4</div>
                <div class="button-div">5</div>
                <div class="button-div">6</div>
                <div class="button-div">1</div>
                <div class="button-div">2</div>
                <div class="button-div">3</div>
                <div class="button-div equalsign">=</div>
                <div class="button-div zero">0</div>
                <div class="button-div">.</div>
            </div>
        </div>
    </body>
</html>